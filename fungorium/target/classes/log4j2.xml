<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <CustomLevels>
        <CustomLevel name="CREATE" intLevel="401"/>
        <CustomLevel name="INIT" intLevel="402"/>
        <CustomLevel name="ADD" intLevel="403"/>
        <CustomLevel name="MOVE" intLevel="404"/>
        <CustomLevel name="ATTACK" intLevel="405"/>
        <CustomLevel name="INJURE" intLevel="406"/>
        <CustomLevel name="DEATH" intLevel="407"/>
        <CustomLevel name="REMOVE" intLevel="408"/>
        
    </CustomLevels>
    
    
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
        </Console>
    </Appenders>

    <Loggers>
        <Root level="trace">
            <AppenderRef ref="console" />
        </Root>
    </Loggers>
</Configuration>
